{
    "name": "generic-goto",
    "displayName": "Generic Goto",
    "description": "Go to definition, open file, open link, etc.",
    "version": "0.0.1",
    "license": "MIT",
    "publisher": "ilmaria",
    "repository": {
        "type": "git",
        "url": "https://github.com/ilmaria/vscode-generic-goto.git"
    },
    "engines": {
        "vscode": "^1.34.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.generic-goto",
                "title": "Generic Goto"
            }
        ],
        "keybindings": [
            {
                "command": "extension.generic-goto",
                "key": "ctrl+shift+alt+f12",
                "mac": "cmd+shift+alt+f12",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.3.1",
        "vscode": "^1.1.28",
        "tslint": "^5.12.1",
        "@types/node": "^10.12.21",
        "@types/mocha": "^2.2.42"
    }
}
